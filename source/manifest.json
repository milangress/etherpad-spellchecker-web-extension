{
  "name": "Ether-Spell",
  "version": "1.0.0",
  "description": "Activate spellchecking in Etherpad",
  "homepage_url": "https://github.com/milangress/etherpad-spellchecker-web-extension",
  "manifest_version": 3,
  "minimum_chrome_version": "110",
  "browser_specific_settings": {
	"gecko": {
	  "id": "ether-spell@milangress.github.io",
	  "strict_min_version": "110.0"
	}
  },
  "icons": {
	"32": "icon32.png",
	"48": "icon48.png",
	"128": "icon128.png"
  },
  "action": {
	"default_popup": "popup.html"
  },
  "permissions": [
	"scripting",
	"activeTab",
	"storage"
  ]
}
