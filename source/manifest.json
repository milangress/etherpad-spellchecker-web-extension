{
  "name": "Ã„h? Ether-Spell",
  "version": "1.0.0",
  "description": "Etherpad spellchecking support browser extension",
  "homepage_url": "https://yopad.eu/p/testing-spelling",
  "manifest_version": 3,
  "minimum_chrome_version": "110",
  "browser_specific_settings": {
	"gecko": {
	  "id": "ether-spell@milangress.github.io",
	  "strict_min_version": "110.0"
	}
  },
  "icons": {
	"128": "icon.png"
  },
  "action": {
	"default_popup": "popup.html"
  },
  "permissions": [
	"scripting",
	"activeTab",
	"storage"
  ],
  "content_scripts": [
	{
	  "matches": [
		"https://*/*"
	  ],
	  "js": [
		"content.js"
	  ],
	  "css": [
		"content.css"
	  ],
	  "run_at": "document_end"
	}
  ]
}
