{
  "name": "EtherSpelling",
  "version": "0.0.0",
  "description": "An awesome new browser extension",
  "homepage_url": "https://yopad.eu/p/testing-spelling",
  "manifest_version": 3,
  "minimum_chrome_version": "100",
  "browser_specific_settings": {
	"gecko": {
	  "id": "awesome-extension@notlmn.github.io",
	  "strict_min_version": "100.0"
	}
  },
  "icons": {
	"128": "icon.png"
  },
  "action": {
	"default_popup": "popup.html"
  },
  "permissions": [
	"scripting",
	"activeTab",
	"storage"
  ],
  "content_scripts": [
	{
	  "matches": [
		"https://*/*"
	  ],
	  "js": [
		"content.js"
	  ],
	  "css": [
		"content.css"
	  ],
	  "run_at": "document_end"
	}
  ]
}
